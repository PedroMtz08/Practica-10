practica10for.py
-----------------------------------------------------------------------------------------------------------------------------------------
"""
For para listas
"""
def forlist():
    for x in [1, 2, 3, 4, 5]:
        print(x)

    for x in ["uno", "dos", "tres", "cuatro", "cinco"]:
        print(x)

"""
For para rangos 
"""
def forrange():
    for x in range(5):
        print(x)

    for y in range(-3,3):
        print(y)
    
    for z in range (-4, 2, 2):
        print(z)

    for i in range(5, 0, -1):  #For inverso -1
        print(i)

    """
    For para diccionarios 
    """ 
def Fordic():
    diccionario = {'manzana': 1, 'pera': 3, 'uva': 10}
    for clave, valor in diccionario.items():
        print(clave," = ",valor)
    
    for clave in diccionario.keys():
        print(clave)

    for valor in diccionario.values():
        print(valor)

    for idx, x in enumerate(diccionario):
        print("El indice {} del elemento {} ".format(idx, x))

"""
Else de for
"""

def elsefor():
    for x in range(5):
        print(x)
    else:
        print("La cuenta se termino")

def elsedor2():
    for x in range(5):
        print(x)
        if x == 2:
            break 
    else:
        print("La cuenta se termino")

def factorial(n):
    temp = 1
    for i in range(2,n+1):
        temp = temp * i
        print(temp)   #Profe una duda, no se porque me imprime un None al final del factorial :(

if __name__ == "__main__":
    forlist()
    forrange()
    Fordic()
    elsefor()
    elsedor2()
    n = int(input("Ingresa numero"))
    print(factorial(n))
-----------------------------------------------------------------------------------------------------------------------------------------
practica10while.py
-----------------------------------------------------------------------------------------------------------------------------------------
def factorial(n):
    i = 2
    temp = 1
    while i <= n:
        temp = temp * i
        i = i+1
    return temp

if __name__ == "__main__":
    a = int(input("Ingresa un numero:"))
    print(factorial(a))
