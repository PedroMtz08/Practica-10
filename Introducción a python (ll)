cola.py
-------------------------------------------------------------------------------------------------------------------------------------------
def encolar(lista,dato):
    lista.append(dato)

def desencolar(lista):
    dato = lista.pop(0) #Es posicion 0 para que borre el primero, sino borrara el ultimo

def imprimir_cola(lista):
    lista.reverse() 
    for x in lista:
        print(x)
    lista.reverse()

def main():
    cola = [0]

    encolar(cola,3)
    encolar(cola,4)
    imprimir_cola(cola)
    print()
    encolar(cola,5)
    imprimir_cola(cola)
    print()
    desencolar(cola)
    imprimir_cola(cola)
    print()
    desencolar(cola)
    imprimir_cola(cola)
    print()

if __name__ == "__main__":
    main()
